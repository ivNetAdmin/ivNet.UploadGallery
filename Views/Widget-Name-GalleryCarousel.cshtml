@{
    Script.Require("ivNet.GalleryCarousel");
}
<section class="col-md-12">
    <div id="CarouselHolder">

        <div id="listingCarousel" class="carousel slide" data-interval="5000" data-ride="carousel" data-ng-controllerx="CarouselController">
            <!-- Carousel indicators -->
            <ol class="carousel-indicators"></ol>
            <!-- Wrapper for carousel items -->
            <div class="carousel-inner">

            </div>
            <!-- Carousel controls --> <a class="carousel-control left" href="#listingCarousel" data-slide="prev"> <span class="glyphicon glyphicon-chevron-left"></span> </a> <a class="carousel-control right" href="#myCarousel" data-slide="next"> <span class="glyphicon glyphicon-chevron-right"></span> </a>
        </div>

    </div>
</section>